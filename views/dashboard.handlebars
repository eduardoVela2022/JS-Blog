{{! List Of Posts }}
<h1 class="row main-container-title">Your Posts</h1>

<ul class="column dashboard-post-list">
  {{! Post }}
  {{#each posts as |post|}}
    <li class="primary-container row dashboard-post-list-item">
      {{! Post Title }}
      <h4>{{post.title}}</h4>

      {{! Post Owner }}
      <h5>Created on </h5>
    </li>
  {{/each}}
</ul>